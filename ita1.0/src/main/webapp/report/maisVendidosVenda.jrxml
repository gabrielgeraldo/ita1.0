	<parameter name="DATA_INI" class="java.sql.Timestamp"/>
	<parameter name="DATA_FIM" class="java.sql.Timestamp"/>
	<queryString>
		<![CDATA[SELECT PRODUTO.CODIGO AS CODIGO,
       PRODUTO.DESCRICAO AS DESCRICAO,
       CATEGORIA.DESCRICAO AS CATEGORIA,
       SUM(QUANTIDADE) AS QUANTIDADE_VENDIDA,
       SUM(ITEM_VENDA.PRECOVENDA) AS TOTAL,
       PRODUTO.PRECOCUSTO * SUM(QUANTIDADE) AS CUSTO_ATUAL,
       SUM(ITEM_VENDA.PRECOVENDA) - PRODUTO.PRECOCUSTO * SUM(QUANTIDADE) AS LUCRO,
       ((SUM(ITEM_VENDA.PRECOVENDA) - PRODUTO.PRECOCUSTO * SUM(QUANTIDADE)) / SUM(ITEM_VENDA.PRECOVENDA) * 100) AS MARGEM_LUCRO
FROM ITEM_VENDA
    INNER JOIN VENDA ON
         ITEM_VENDA.VENDA_CODIGO = VENDA.CODIGO
    INNER JOIN PRODUTO ON
         ITEM_VENDA.PRODUTO_CODIGO = PRODUTO.CODIGO
    INNER JOIN CATEGORIA ON
         CATEGORIA.CODIGO = CATEGORIA_CODIGO
WHERE DATE(VENDA.DATA) BETWEEN $P{DATA_INI}  AND $P{DATA_FIM} 
GROUP BY PRODUTO.CODIGO, PRODUTO.DESCRICAO, CATEGORIA.DESCRICAO, PRODUTO.PRECOCUSTO
ORDER BY CATEGORIA.DESCRICAO]]>
	</queryString>
